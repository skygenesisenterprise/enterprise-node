{
  "name": "@skygenesisenterprise/eslint-config",
  "version": "1.0.9",
  "description": "Enterprise ESLint Configuration - Configuration ESLint pour les projets Enterprise",
  "type": "module",
  "private": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "enterprise",
    "sky-genesis"
  ],
  "author": "Sky Genesis Enterprise",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/eslint": "^8.0.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skygenesisenterprise/enterprise.git",
    "directory": "packages/development/eslint-config"
  },
