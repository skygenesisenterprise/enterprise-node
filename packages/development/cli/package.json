{
  "name": "@skygenesisenterprise/cli",
  "version": "0.1.0",
  "description": "Enterprise CLI - Outils en ligne de commande pour le SDK Enterprise",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "enterprise": "./dist/bin/enterprise.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "cli",
    "enterprise",
    "sdk",
    "tools",
    "sky-genesis"
  ],
  "author": "Sky Genesis Enterprise",
  "license": "MIT",
  "dependencies": {
    "@skygenesisenterprise/shared": "workspace:*",
    "commander": "^11.0.0",
    "chalk": "^5.0.0",
    "inquirer": "^9.0.0",
    "ora": "^7.0.0",
    "fs-extra": "^11.0.0",
    "glob": "^10.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skygenesisenterprise/enterprise.git",
    "directory": "packages/development/cli"
  },
  "publishConfig": {
    "access": "public"
  }
