{
  "name": "@skygenesisenterprise/enterprise-node",
  "version": "1.0.9",
  "description": "Enterprise SDK Monorepo - Le runtime web du SDK universel Enterprise de Sky Genesis Enterprise",
  "private": false,
  "workspaces": [
    "packages/*",
    "packages/modules/*",
    "packages/integrations/*",
    "packages/development/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=!@skygenesisenterprise/cli && changeset publish"
  },
  "keywords": [
    "sdk",
    "enterprise",
    "monorepo",
    "wasm",
    "ai",
    "storage",
    "ui",
    "auth",
    "project-management",
    "skygenesisenterprise"
  ],
  "author": "Sky Genesis Enterprise",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.js"
    },
    "./autocomplete": {
      "types": "./src/autocomplete-system.d.ts",
      "import": "./src/autocomplete-system.js"
    },
    "./snippets": {
      "types": "./src/autocomplete.d.ts",
      "import": "./src/autocomplete.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "dist/",
    "src/",
    "packages/"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "turbo": "^1.13.4",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.10.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/skygenesisenterprise/enterprise-node.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
